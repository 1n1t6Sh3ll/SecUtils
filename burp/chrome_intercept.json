{
    "proxy":{
        "intercept_client_requests":{
            "automatically_fix_missing_or_superfluous_new_lines_at_end_of_request":false,
            "automatically_update_content_length_header_when_the_request_is_edited":true,
            "do_intercept":true,
            "rules":[
                {
                    "boolean_operator":"and",
                    "enabled":true,
                    "match_condition":"(^gif$|^jpg$|^png$|^css$|^js$|^ico$)",
                    "match_relationship":"does_not_match",
                    "match_type":"file_extension"
                },
                {
                    "boolean_operator":"or",
                    "enabled":false,
                    "match_relationship":"contains_parameters",
                    "match_type":"request"
                },
                {
                    "boolean_operator":"or",
                    "enabled":false,
                    "match_condition":"(get|post)",
                    "match_relationship":"does_not_match",
                    "match_type":"http_method"
                },
                {
                    "boolean_operator":"and",
                    "enabled":false,
                    "match_relationship":"is_in_target_scope",
                    "match_type":"url"
                },
                {
                    "boolean_operator":"and",
                    "enabled":true,
                    "match_condition":"^.*client.*\\.google\\.com$",
                    "match_relationship":"does_not_match",
                    "match_type":"domain_name"
                },
                {
                    "boolean_operator":"and",
                    "enabled":true,
                    "match_condition":"^gitcdn\\.xyz$",
                    "match_relationship":"does_not_match",
                    "match_type":"domain_name"
                },
                {
                    "boolean_operator":"and",
                    "enabled":true,
                    "match_condition":"^raw\\.githubusercontent\\.com$",
                    "match_relationship":"does_not_match",
                    "match_type":"domain_name"
                },
                {
                    "boolean_operator":"and",
                    "enabled":true,
                    "match_condition":"^fonts\\.gstatic\\.com$",
                    "match_relationship":"does_not_match",
                    "match_type":"domain_name"
                },
                {
                    "boolean_operator":"and",
                    "enabled":true,
                    "match_condition":"^lastpass\\.com$",
                    "match_relationship":"does_not_match",
                    "match_type":"domain_name"
                },
                {
                    "boolean_operator":"and",
                    "enabled":true,
                    "match_condition":"^loglogin\\.lastpass\\.com$",
                    "match_relationship":"does_not_match",
                    "match_type":"domain_name"
                },
                {
                    "boolean_operator":"and",
                    "enabled":true,
                    "match_condition":"^www\\.google\\.com$",
                    "match_relationship":"does_not_match",
                    "match_type":"domain_name"
                },
                {
                    "boolean_operator":"and",
                    "enabled":true,
                    "match_condition":"^.*\\.googleusercontent\\.com$",
                    "match_relationship":"does_not_match",
                    "match_type":"domain_name"
                },
                {
                    "boolean_operator":"and",
                    "enabled":true,
                    "match_condition":"^.*\\.gstatic\\.com$",
                    "match_relationship":"does_not_match",
                    "match_type":"domain_name"
                },
                {
                    "boolean_operator":"and",
                    "enabled":true,
                    "match_condition":"^mail\\.google\\.com$",
                    "match_relationship":"does_not_match",
                    "match_type":"domain_name"
                },
                {
                    "boolean_operator":"and",
                    "enabled":true,
                    "match_condition":"^.*\\.google\\.com$",
                    "match_relationship":"does_not_match",
                    "match_type":"domain_name"
                },
                {
                    "boolean_operator":"and",
                    "enabled":true,
                    "match_condition":"^.*\\.googleapis\\.com$",
                    "match_relationship":"does_not_match",
                    "match_type":"domain_name"
                }
            ]
        }
    }
}